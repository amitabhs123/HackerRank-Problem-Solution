public class Demo1{
	public static void main(String [] args) {
		Scanner s=new Scanner(System.in);
		int z=s.nextInt();
	for(int k=1;k<(z+1);k++) {	
		for(int i=0;i<z-k;i++) {
			System.out.print(" ");
		}
		
		
		for(int j=0;j<k;j++) {
			System.out.print("#");
		}
		System.out.println();
	}
	}	
}
